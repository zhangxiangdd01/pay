{
    "name": "paycenter/pay",
    "description": "专注 Alipay 和 WeChat 的支付扩展包",
    "keywords": ["alipay", "wechat", "pay"],
    "type": "library",
  
  
    "require": {
        "php": ">=7.0",
        "ext-openssl": "*",
        "ext-simplexml":"*",
        "monolog/monolog": "^1.23",
        "symfony/http-foundation": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "mockery/mockery": "1.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Yansongda\\Pay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yansongda\\Pay\\Tests\\": "tests"
        }
    },
    "license": "MIT"
}
